{
    "name": "openapi-sdk/demo",
    "description": "OpenAPI SDK Demo for PHP",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ratchet/pawl": "^0.4.1",
        "react/event-loop": "^1.2",
        "react/promise": "^2.9",
        "react/socket": "^1.12",
        "react/stream": "^1.2",
        "guzzlehttp/psr7": "^2.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenApiSdk\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
} 